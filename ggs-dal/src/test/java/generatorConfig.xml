<?xml version="1.0" encoding="UTF-8"?>    
<!DOCTYPE generatorConfiguration    
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"    
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- mybatis-generator:generate -->
	<!-- 到本文件的目录下,执行如下cmd命令 -->
	<!-- java -jar mybatis-generator-core-1.3.2.jar -configfile generatorConfig.xml -overwrite -->
	<!-- 数据库驱动 -->
	<!-- <classPathEntry location="mysql-connector-java-5.1.35.jar" /> -->

	<!-- 根据自己本地实际的jar位置修改 -->
	<!--<classPathEntry location="/home/nyh/.m2/repository/mysql/mysql-connector-java/5.1.33/mysql-connector-java-5.1.33.jar" />-->
	<classPathEntry location="/Users/wungmc/mvn_repository/mysql/mysql-connector-java/5.1.33/mysql-connector-java-5.1.33.jar" />

	<!-- maven 自动生成代码  命令(在 ggs-dal 目录下)：
			mvn mybatis-generator:generate
	 -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库链接URL，用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/ggs"
						userId="root" password="root">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="io.chicken.ggs.dal.model" targetProject="../ggs-dal/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 生成映射文件的包名和位置 -->
		<sqlMapGenerator targetPackage="io.chicken.ggs.dal.dao" targetProject="../ggs-dal/src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="io.chicken.ggs.dal.dao" targetProject="../ggs-dal/src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>



        <table tableName="notice_readed" domainObjectName="NoticeReaded" enableCountByExample="false" enableUpdateByExample="false"
           enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>

	</context>
</generatorConfiguration> 